  "name": "travelbuddy",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "cross-env NODE_ENV=development expo start --go",
    "start:dev": "expo start --dev-client",
    "android": "expo start --android --go",
    "android:dev": "expo start --android --dev-client",
    "web": "expo start --web --go",
    "test": "jest --watchAll",
    "build:dev": "dotenv -e .env -e .env.development -- eas build --platform android --profile development --local",
    "build:prev": "dotenv -e .env -e .env.preview -- eas build --platform android --profile preview --local",
    "build:prod": "dotenv -- eas build --platform android --profile production --local",
    "lint": "eslint .",
    "lint-fix": "eslint --fix .",
    "prettier": "prettier --write .",
    "mocks": "mocks-server",
    "ios": "expo run:ios"
  },
  "jest": {
    "preset": "jest-expo"
  },
